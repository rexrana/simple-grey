{% extends "base.twig" %}

{% block main %}
  {% for post in posts %}
    {% include['content/teaser-' ~ post.post_type ~ '.twig', 'content/teaser.twig'] %}
  {% endfor %}

  {% if pagination_style == false %}
  {% elseif pagination_style == 'numbered' and numbered_nav %}
      {% include 'navigation/numbered-nav.twig' %}
  {% else %}
    {% include 'navigation/paging-nav.twig' %}
  {% endif %}
{% endblock %}
